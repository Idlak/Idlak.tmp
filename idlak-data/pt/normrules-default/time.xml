        </comment>
      <match>
        <rgx id="m0" name="time_midday" src="lcase" offset="0"/>
      </match>
      <replace>
        <cpy val="meán lae" offset="0"/>
        <cpy val="" offset="0"/>
      </replace>
    </rule>

    <rule name="time_midday_split">
      <comment>
            12-00 -&gt; meán lae
