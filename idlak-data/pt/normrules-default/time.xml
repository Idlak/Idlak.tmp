<nrules>

  <rules>

    <rule name="time_midnight">
      <comment>
            00:00 -&gt; meán oíche
        </comment>
      <match>
        <rgx id="m0" name="time_midnight" src="lcase" offset="0"/>
      </match>
      <replace>
        <cpy val="meán oíche" offset="0"/>
      </replace>
    </rule>

    <rule name="time_midnight_split">
      <comment>
            00-00 -&gt; meán oíche
        </comment>
      <match>
        <rgx id="m0" name="two_zeros" src="lcase" offset="0"/>
        <rgx id="m1" name="is_hyphen" src="lcase" offset="1"/>
        <rgx id="m2" name="two_zeros" src="lcase" offset="2"/>
      </match>
      <replace>
        <cpy val="meán oíche" offset="0"/>
        <cpy val="" offset="1"/>
        <cpy val="" offset="2"/>
      </replace>
    </rule>

    <rule name="time_midnight_12">
      <comment>
            12:00AM -&gt; meán oíche
        </comment>
      <match>
        <rgx id="m0" name="time_midnight_12" src="lcase" offset="0"/>
      </match>
      <replace>
        <cpy val="meán oíche" offset="0"/>
        <cpy val="" offset="0"/>
      </replace>
    </rule>

    <rule name="time_midday">
